      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false
    },
    "//#lint": {},
    "//#format:check": {},
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "test:ci": {}
  }
}
